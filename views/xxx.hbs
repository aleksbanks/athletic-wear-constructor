<h2>Гетры</h2>
<table>
  <tr>
    <th>Цвет</th>
    <th>Размер</th>
  </tr>
    {{#each allSocks}}
  <tr>
      <td>{{this.color}}</td>
      <td>{{size}}</td>
  </tr>
    {{/each}}
</table>
